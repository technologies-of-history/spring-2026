<div style="margin: 2rem 0;">
  <link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" integrity="sha256-p4NxAoJBhIIN+hmNHrzRCf9tD/miZyoHS5obTRR9BMY=" crossorigin=""/>

  <div id="map" style="position: relative; height: 500px; width: 100%;"></div>
  <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js" integrity="sha256-20nQCchB9co0qIjJZRGuk2/Z9VM+kNiyxNV1lvTlZBo=" crossorigin=""></script>
    <script>
    const map = L.map('map').setView([46.60, 1.51], 6);
    L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
    maxZoom: 19,
    attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a>'
}).addTo(map);
var marker = L.marker([46.59, 1.52]).addTo(map).bindPopup('Argentomagus').openPopup();
var marker = L.marker([43.66, 0.52]).addTo(map).bindPopup('Ausci').openPopup();
var marker = L.marker([47.08, 2.34]).addTo(map).bindPopup('Avaricum').openPopup();
var marker = L.marker([44.84, -0.58]).addTo(map).bindPopup('Burdigala').openPopup();
var marker = L.marker([46.65, 4.57]).addTo(map).bindPopup('Genoliacum').openPopup();
var marker = L.marker([43.03, 0.57]).addTo(map).bindPopup('Lugdunum Convenarum').openPopup();
var marker = L.marker([45.74, -0.63]).addTo(map).bindPopup('Mediolanum Santonum').openPopup();
var marker = L.marker([45.52, -0.86]).addTo(map).bindPopup('Tamnum').openPopup();
</script>
</div>
</html>

