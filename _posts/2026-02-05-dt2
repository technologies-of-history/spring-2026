  <div id="my-mirador" style="position: relative; height: 500px; width: 100%;"></div>
  
  <script src="https://unpkg.com/mirador@latest/dist/mirador.min.js"></script>
    <script>
      const mirador = Mirador.viewer({
        id: "my-mirador",
        manifests: {
          "http://edh.ub.uni-heidelberg.de/edh/inschrift/HD058206": { provider: "Epigraphic Database Heidelberg" }
        },
        windows: [
          { loadedManifest: "http://edh.ub.uni-heidelberg.de/edh/inschrift/HD058206", canvasIndex: 0, thumbnailNavigationPosition: 'none' }
        ]
      })();
</script>
